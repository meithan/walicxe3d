<a name="l00657"></a>00657   <span class="keyword">write</span>(<a class="code" href="namespaceglobals.html#a5baaf3368c41942a3e5e48a39b97c2ce">logu</a>,<span class="stringliteral">&#39;(1x,a,i0,a,a,a)&#39;</span>) <span class="stringliteral">&quot;Rank &quot;</span>, <a class="code" href="namespaceglobals.html#a93f283242b146b3ef8b53fd1b1ad7bac">rank</a>, <span class="stringliteral">&quot; writing GRID meta file &quot;</span>, trim(gridfile), <span class="stringliteral">&quot; ...&quot;</span>
<a name="l00658"></a>00658      
<a name="l00659"></a>00659   <span class="comment">! VTK Header</span>
<a name="l00660"></a>00660   <span class="keyword">write</span>(unitout) <span class="stringliteral">&quot;# vtk DataFile Version 4.2&quot;</span>, lf
<a name="l00661"></a>00661   <span class="keyword">write</span>(unitout) <span class="stringliteral">&quot;Output from Walicxe3D&quot;</span>, lf
<a name="l00662"></a>00662   <span class="keyword">write</span>(unitout) <span class="stringliteral">&quot;BINARY&quot;</span>, lf
<a name="l00663"></a>00663   <span class="keyword">write</span>(unitout) <span class="stringliteral">&quot;DATASET UNSTRUCTURED_GRID&quot;</span>, lf
<a name="l00664"></a>00664 
<a name="l00665"></a>00665   <span class="comment">! The number of vtk &quot;cells&quot; is the number of blocks (all processors)</span>
<a name="l00666"></a>00666   <span class="comment">! and there are 8 vtk &quot;points&quot; per block</span>
<a name="l00667"></a>00667   npoints = <a class="code" href="namespaceglobals.html#aea9a3f8b9aca242686487ccd26a299ba">nbActive</a>*8
<a name="l00668"></a>00668   ncells = <a class="code" href="namespaceglobals.html#aea9a3f8b9aca242686487ccd26a299ba">nbActive</a>
<a name="l00669"></a>00669 
<a name="l00670"></a>00670   <span class="comment">! VTK cell vertices list</span>
<a name="l00671"></a>00671   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(a,1x,i12,1x,a)&#39;</span>) <span class="stringliteral">&quot;POINTS&quot;</span>, npoints, <span class="stringliteral">&quot;float&quot;</span>
<a name="l00672"></a>00672   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00673"></a>00673   counter = 0
<a name="l00674"></a>00674   <span class="keyword">do</span> nb=1,<a class="code" href="namespaceparameters.html#a9956bfd2c200d8b3dd24e4294ed11770">nbMaxGlobal</a>
<a name="l00675"></a>00675     bID = <a class="code" href="namespaceglobals.html#ae1b47be87361ae69b73a7cdc15511687">globalBlocks</a>(nb)
<a name="l00676"></a>00676     <span class="keyword">if</span> (bID.ne.-1) <span class="keyword">then</span>
<a name="l00677"></a>00677 <span class="comment">!        print*, &quot;Writing block&quot;, bID, &quot;to grid&quot;  !DEBUG</span>
<a name="l00678"></a>00678       counter = counter + 1
<a name="l00679"></a>00679       call <a class="code" href="admesh_8f90.html#a971d8fccc948f21cfb43c8b9dd2ade5a">meshlevel</a>(bID, lvl)
<a name="l00680"></a>00680       call <a class="code" href="admesh_8f90.html#af40e152b0f3f3b052718add6289b47dc">getRefCorner</a>(bID, xb, yb, zb)
<a name="l00681"></a>00681 <span class="comment">!        print*, &quot;Writing block&quot;, bID</span>
<a name="l00682"></a>00682 <span class="comment">!        print*, &quot;Ref corner:&quot;, xb, yb, zb</span>
<a name="l00683"></a>00683       <span class="comment">! For each block, write its eight corners</span>
<a name="l00684"></a>00684       <span class="keyword">do</span> k=0,1
<a name="l00685"></a>00685         <span class="keyword">do</span> j=0,1
<a name="l00686"></a>00686           <span class="keyword">do</span> i=0,1
<a name="l00687"></a>00687             <span class="keyword">if</span> (units_type.eq.PHYS_UNITS) <span class="keyword">then</span>
<a name="l00688"></a>00688               xx = ( xb + i*<a class="code" href="namespaceparameters.html#ad032cf1de9e7d232d045a52ece9eae4b">ncells_x</a>*<a class="code" href="namespaceglobals.html#a4b0c55fb2db0260908cd128c2a6f762f">dx</a>(lvl) ) * <a class="code" href="namespaceparameters.html#a522e0ca69e61a46d801700b9a9ec9562">l_sc</a>
<a name="l00689"></a>00689               yy = ( yb + j*<a class="code" href="namespaceparameters.html#aa52d0561bd220fc5f155b7d1932d8f1d">ncells_y</a>*<a class="code" href="namespaceglobals.html#a75a803e9fb814f0f532f09795c31948f">dy</a>(lvl) ) * <a class="code" href="namespaceparameters.html#a522e0ca69e61a46d801700b9a9ec9562">l_sc</a>
<a name="l00690"></a>00690               zz = ( zb + k*<a class="code" href="namespaceparameters.html#a568fe0267a053f4e31fc851dba5a1ede">ncells_z</a>*<a class="code" href="namespaceglobals.html#a359c03a4b72ff3ab06d5fe9fbc810f9a">dz</a>(lvl) ) * <a class="code" href="namespaceparameters.html#a522e0ca69e61a46d801700b9a9ec9562">l_sc</a>
<a name="l00691"></a>00691             <span class="keyword">else</span> <span class="keyword">if</span> (units_type.eq.CODE_UNITS) <span class="keyword">then</span>
<a name="l00692"></a>00692               xx = ( xb + i*<a class="code" href="namespaceparameters.html#ad032cf1de9e7d232d045a52ece9eae4b">ncells_x</a>*<a class="code" href="namespaceglobals.html#a4b0c55fb2db0260908cd128c2a6f762f">dx</a>(lvl) )
<a name="l00693"></a>00693               yy = ( yb + j*<a class="code" href="namespaceparameters.html#aa52d0561bd220fc5f155b7d1932d8f1d">ncells_y</a>*<a class="code" href="namespaceglobals.html#a75a803e9fb814f0f532f09795c31948f">dy</a>(lvl) )
<a name="l00694"></a>00694               zz = ( zb + k*<a class="code" href="namespaceparameters.html#a568fe0267a053f4e31fc851dba5a1ede">ncells_z</a>*<a class="code" href="namespaceglobals.html#a359c03a4b72ff3ab06d5fe9fbc810f9a">dz</a>(lvl) )
<a name="l00695"></a>00695             <span class="keyword">end if</span>
<a name="l00696"></a>00696             <span class="keyword">write</span>(unitout) xx, yy, zz
<a name="l00697"></a>00697           <span class="keyword">end do</span>
<a name="l00698"></a>00698         <span class="keyword">end do</span>
<a name="l00699"></a>00699       <span class="keyword">end do</span>
<a name="l00700"></a>00700     <span class="keyword">end if</span>
<a name="l00701"></a>00701   <span class="keyword">end do</span>
<a name="l00702"></a>00702   <span class="keyword">write</span>(unitout) lf
<a name="l00703"></a>00703   <span class="keyword">write</span>(<a class="code" href="namespaceglobals.html#a5baaf3368c41942a3e5e48a39b97c2ce">logu</a>,<span class="stringliteral">&#39;(a,i6)&#39;</span>) <span class="stringliteral">&quot; Number of global active blocks: &quot;</span>, counter
<a name="l00704"></a>00704 
<a name="l00705"></a>00705   <span class="comment">! VTK cell descriptions (i.e., which vertices define each cell)</span>
<a name="l00706"></a>00706   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(a,1x,i12,1x,i12)&#39;</span>) <span class="stringliteral">&quot;CELLS&quot;</span>, ncells, ncells*(8+1)
<a name="l00707"></a>00707   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00708"></a>00708 <span class="comment">!    print*, &quot;CELLS &quot;, ncells, ncells*(8+1)    !DEBUG</span>
<a name="l00709"></a>00709   p = 0
<a name="l00710"></a>00710   <span class="keyword">do</span> nb=1,<a class="code" href="namespaceparameters.html#a9956bfd2c200d8b3dd24e4294ed11770">nbMaxGlobal</a>
<a name="l00711"></a>00711     bID = <a class="code" href="namespaceglobals.html#ae1b47be87361ae69b73a7cdc15511687">globalBlocks</a>(nb)
<a name="l00712"></a>00712     <span class="keyword">if</span> (bID.ne.-1) <span class="keyword">then</span>
<a name="l00713"></a>00713       <span class="keyword">write</span>(unitout) 8, p, p+1, p+2, p+3, p+4, p+5, p+6, p+7
<a name="l00714"></a>00714       p = p + 8
<a name="l00715"></a>00715     <span class="keyword">end if</span>
<a name="l00716"></a>00716   <span class="keyword">end do</span>
<a name="l00717"></a>00717   <span class="keyword">write</span>(unitout) lf
<a name="l00718"></a>00718 
<a name="l00719"></a>00719   <span class="comment">! VTK cell types - all are voxels, type=11</span>
<a name="l00720"></a>00720   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(a,1x,i12)&#39;</span>) <span class="stringliteral">&quot;CELL_TYPES&quot;</span>, ncells
<a name="l00721"></a>00721   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00722"></a>00722   <span class="keyword">do</span> nb=1,<a class="code" href="namespaceparameters.html#a9956bfd2c200d8b3dd24e4294ed11770">nbMaxGlobal</a>
<a name="l00723"></a>00723     bID = <a class="code" href="namespaceglobals.html#ae1b47be87361ae69b73a7cdc15511687">globalBlocks</a>(nb)
<a name="l00724"></a>00724     <span class="keyword">if</span> (bID.ne.-1) <span class="keyword">then</span>
<a name="l00725"></a>00725       <span class="keyword">write</span>(unitout) 11
<a name="l00726"></a>00726     <span class="keyword">end if</span>
<a name="l00727"></a>00727   <span class="keyword">end do</span>
<a name="l00728"></a>00728   <span class="keyword">write</span>(unitout) lf
<a name="l00729"></a>00729 
<a name="l00730"></a>00730   <span class="comment">! VTK cell &quot;data&quot; begins here: ranks and bIDs</span>
<a name="l00731"></a>00731   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(A,I12)&#39;</span>) <span class="stringliteral">&quot;CELL_DATA&quot;</span>, ncells
<a name="l00732"></a>00732   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00733"></a>00733 
<a name="l00734"></a>00734   <span class="comment">! Owner&#39;s rank</span>
<a name="l00735"></a>00735   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(A)&#39;</span>)  <span class="stringliteral">&quot;SCALARS rank integer 1&quot;</span>
<a name="l00736"></a>00736   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00737"></a>00737   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(a)&#39;</span>)  <span class="stringliteral">&quot;LOOKUP_TABLE default&quot;</span>
<a name="l00738"></a>00738   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00739"></a>00739   <span class="keyword">do</span> nb=1,<a class="code" href="namespaceparameters.html#a9956bfd2c200d8b3dd24e4294ed11770">nbMaxGlobal</a>
<a name="l00740"></a>00740     bID = <a class="code" href="namespaceglobals.html#ae1b47be87361ae69b73a7cdc15511687">globalBlocks</a>(nb)
<a name="l00741"></a>00741     <span class="keyword">if</span> (bID.ne.-1) <span class="keyword">then</span>
<a name="l00742"></a>00742       call <a class="code" href="admesh_8f90.html#a5423257f930e1a76bcf559ca03bf74c6">getOwner </a>(bID, owner)
<a name="l00743"></a>00743       <span class="keyword">write</span>(unitout) owner
<a name="l00744"></a>00744     <span class="keyword">end if</span>
<a name="l00745"></a>00745   <span class="keyword">end do</span>
<a name="l00746"></a>00746   <span class="keyword">write</span>(unitout) lf
<a name="l00747"></a>00747 
<a name="l00748"></a>00748   <span class="comment">! Block&#39;s bID</span>
<a name="l00749"></a>00749   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(A)&#39;</span>)  <span class="stringliteral">&quot;SCALARS bID integer 1&quot;</span>
<a name="l00750"></a>00750   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00751"></a>00751   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(a)&#39;</span>)  <span class="stringliteral">&quot;LOOKUP_TABLE default&quot;</span>
<a name="l00752"></a>00752   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00753"></a>00753   <span class="keyword">do</span> nb=1,<a class="code" href="namespaceparameters.html#a9956bfd2c200d8b3dd24e4294ed11770">nbMaxGlobal</a>
<a name="l00754"></a>00754     bID = <a class="code" href="namespaceglobals.html#ae1b47be87361ae69b73a7cdc15511687">globalBlocks</a>(nb)
<a name="l00755"></a>00755     <span class="keyword">if</span> (bID.ne.-1) <span class="keyword">then</span>
<a name="l00756"></a>00756       <span class="keyword">write</span>(unitout) bID
<a name="l00757"></a>00757     <span class="keyword">end if</span>
<a name="l00758"></a>00758   <span class="keyword">end do</span>
<a name="l00759"></a>00759   <span class="keyword">write</span>(unitout) lf
<a name="l00760"></a>00760 
<a name="l00761"></a>00761   <span class="comment">! Hilbert Key</span>
<a name="l00762"></a>00762   maxnb = max(<a class="code" href="namespaceglobals.html#a295174106a6216b803f676d41c98c655">nbx</a>(<a class="code" href="namespaceglobals.html#a05704552a4bcfee67afc52ad008c5a98">maxlev</a>), nby(<a class="code" href="namespaceglobals.html#a05704552a4bcfee67afc52ad008c5a98">maxlev</a>), nbz(<a class="code" href="namespaceglobals.html#a05704552a4bcfee67afc52ad008c5a98">maxlev</a>))
<a name="l00763"></a>00763   order = ceiling(log(1.0*maxnb)/log(2.0))
<a name="l00764"></a>00764   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(A)&#39;</span>)  <span class="stringliteral">&quot;SCALARS hkey integer 1&quot;</span>
<a name="l00765"></a>00765   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00766"></a>00766   <span class="keyword">write</span>(cbuffer,<span class="stringliteral">&#39;(a)&#39;</span>)  <span class="stringliteral">&quot;LOOKUP_TABLE default&quot;</span>
<a name="l00767"></a>00767   <span class="keyword">write</span>(unitout) trim(cbuffer), lf
<a name="l00768"></a>00768   <span class="keyword">do</span> nb=1,<a class="code" href="namespaceparameters.html#a9956bfd2c200d8b3dd24e4294ed11770">nbMaxGlobal</a>
<a name="l00769"></a>00769     bID = <a class="code" href="namespaceglobals.html#ae1b47be87361ae69b73a7cdc15511687">globalBlocks</a>(nb)
<a name="l00770"></a>00770     <span class="keyword">if</span> (bID.ne.-1) <span class="keyword">then</span>
<a name="l00771"></a>00771       call <a class="code" href="loadbalance_8f90.html#a97694885a870ffa20811f2c8efa05dde">getHilbertKey</a>(bID, hk)
<a name="l00772"></a>00772       hilb = hk
<a name="l00773"></a>00773       <span class="keyword">write</span>(unitout) hilb
<a name="l00774"></a>00774     <span class="keyword">end if</span>
<a name="l00775"></a>00775   <span class="keyword">end do</span>
<a name="l00776"></a>00776   <span class="keyword">write</span>(unitout) lf
<a name="l00777"></a>00777 
<a name="l00778"></a>00778   <span class="keyword">close</span>(unitout)
